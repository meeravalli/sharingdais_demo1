<h3 class="heading_content">Sharingdais List Requirements</h3><br>
  <div class="display padding_five">
         <!-- Nav tabs -->
<ul class="nav nav-tabs" role="tablist" style="float:left">
  <li class="active"><a href="#book" role="tab" data-toggle="tab">Book List Requirements</a></li>
  <li><a href="#food" role="tab" data-toggle="tab">Food List Requirements</a></li>
</ul>

<!-- Tab panes -->
<div class="tab-content">
  <div class="tab-pane active" id="book">
    <div class="display padding_five">
            <table class="table table-hover">
                <thead class="heading">
                    <tr>
                        <th>User Name</th>
                        <th>Locality</th>
                        <th>Food type</th>
                        <th>Region</th>
                        <th>Budget</th>
                        <th>Details</th>
                        <th>Date</th>
                    </tr>
                 </thead>
                 <tbody>
                    <% @food_orders.each do |f| %>
                    <tr>
                        <td>
                            <%= f.user.name %>
                        </td>
                        <td><%= f.location.location_name %></td>
                        <td><%= f.food_type.name %></td>
                        <td>
                            <% if !f.region.nil? %>
                                <%= f.region.name %>
                            <% else %>
                                Not Avaliabel
                            <% end %>
                        </td>
                        <td><%= f.budget %></td>
                        <td><%= f.details %></td>
                        <td><%= f.created_at.strftime("%b %d, %Y") %></td>
                    </tr>
                    <% end %>
                   
                    
                </tbody>
            </table>
                <div class="pagination">
      <%= will_paginate @food_orders %>
            </div>
        </div>
  </div>


  <div class="tab-pane" id="food">
    <div class="display padding_five">
            <table class="table table-hover">
                <thead class="heading">
                    <tr>
                        <th>User Name</th>
                        <th>Book Name</th>
                        <th>Author</th>
                        <th>Region</th>
                        <th>Rent</th>
                        <th>Description</th>
                        <th>Date</th>
                    </tr>
                 </thead>
                 <tbody>
                    <% @book_orders.each do |b| %>
                    <tr>
                        <td>
                            <% if !b.user.nil? %>
                                <%= b.user.name %>
                            <% else %>
                                Account Deleted
                            <% end %>
                        </td>
                        <td><%= b.name %></td>
                        <td><%= b.author %>gg</td>
                        <td><%= b.location.location_name %></a>gg</td>
                        <td><%= b.rent %></td>
                        <td><%= b.description %></td>
                        <td><%= b.created_at.strftime("%b %d, %Y") %></td>
                    </tr>
                    <% end %>
                   
                    
                </tbody>
            </table>
                <div class="pagination">
      <%= will_paginate @book_orders%>
            </div>
        </div>
  </div>
</div> 
  </div>
                
                                  
                          
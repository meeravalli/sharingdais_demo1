<div class="login_page">
<!--<div class="login_img"><%#= image_tag 'newlogo.jpg',  :alt=>""%></div>-->
<div class="content">
<div class="resize">
<div class="signup_content">
<h2 class="login_heading">Signup
<div class="signup_link">Already have an account? <%= link_to "Sign in", new_user_session_path %></div>
</h2>
<div class="signup_form">
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
<%= devise_error_messages! %>
<div class="signup_col_1">
															   <div class="text_lable">Email<i>*</i></div>
    	                            <div class="text_feild"><%= f.email_field :email, :autofocus=> true, :placeholder => 'Enter Email Id', :class => "form-control login_feild", :tabindex => 1  %></div>
																  <div class="text_lable">Password<i>*</i></div>
    	                            <div class="text_feild"><%= f.password_field :password, :autocomplete => "off", :placeholder => 'Enter Password', :class => "form-control login_feild", :tabindex => 2 %></div>
																	<div class="text_lable">Confirm Password<i>*</i></div>
									                 <div class="text_feild"><%= f.password_field :password_confirmation, :autocomplete => "off", :placeholder => 'Enter Confirm Password', :class => "form-control login_feild",:tabindex => 3 %></div>	
</div>
                                
<div class="signup_col_1">
																	<div class="text_lable">Name<i>*</i></div>
    	                            <div class="text_feild"><%= f.text_field :name,  :placeholder=>"Enter your Name", :class=>"form-control login_feild", :tabindex => 4 %></div>
																	<div class="text_lable">Phone<i>*</i></div>
    	                            <div class="text_feild"><%= f.text_field :phone_no,  :placeholder=>"Enter your Phone no.", :class=>"form-control login_feild",  :tabindex => 6 %></div>
																	<div class="text_lable">Photo Upload</div>
    	                            <div class="text_feild"><%= f.file_field :photo, :class=>"login_feild"  %></div>
																	<span  class="upload_dtest">(only .jpg or .jpeg or .png or .gif file)</span>
</div>
                                
<div class="signup_col_full_width margin_none">
                                	<div class="text_lable">Address<i>*</i></div>
    	                            <div class="text_feild"><%= f.text_area :address, :rows => 3, :cols => 25, :placeholder=>"Enter your Address", :class=>"form-control signup_textarea",  :tabindex => 7 %></div>
</div>
<div class="signup_col_full_width margin_none">
    <div class="text_lable">By clicking Sign Up, you agree to our Terms &amp; Conditions and <a href="/legal_terms">Privacy Policy</a>.</div>
    
</div>
<div class="signup_col_full_width">
                                <div class="text_feild"><%= f.submit "Sign up", :class=>"login_button" %></div>
																  <%end%>
</div>
</div>
<div class="sign_links">
															<%= render "devise/shared/links" %>
</div>
													
</div>
</div>
</div>
</div>
		

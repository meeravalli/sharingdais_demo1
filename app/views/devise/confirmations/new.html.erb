<div class="login_page">
<div class="login_img"><%= image_tag 'newlogo.jpg',  :alt=>""%></div>
					<div class="content">
						<div class="resize">
							<div class="login_content">
                        	<h2 class="login_heading">Resend confirmation instructions</h2>
													<div class="login_form">
														<%= form_for(resource, as: resource_name, url: confirmation_path(resource_name), html: { method: :post }) do |f| %>
														<%= devise_error_messages! %>
													<div class="text_lable">Email<i>*</i></div>
													<div class="text_feild"><%= f.email_field :email, :autofocus=> true, :placeholder => 'Enter your Email id', :class => "form-control login_feild"  %></div>
													<div class="text_feild"><%= f.submit "Resend confirmation instructions", :class=>"login_button" %></div>
														<%end%>
													</div>
										<div class="sign_links">
												<%= render "devise/shared/links" %>
										</div>
						</div>
						</div>
					</div>
				</div>
				


